      <property name="styleSheet">
       <string notr="true">#dateEdit {
    border: 1px solid #ccc; /* Same border color */
    border-radius: 8px; /* Match border radius with comboBox_3 */
    background-color: #f9f9f9; /* Same background color as comboBox_3 */
    padding: 0px; /* Adjust padding to match */
    margin: 0px; /* Consistent margin */
    font-size: 16px; /* Match font size */
}

#dateEdit:focus {
    border-color: #dd914f; /* Change border color on focus */
    outline: none; /* Remove outline */
    background-color: #ffffff; /* White background on focus */
}

#dateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right; /* Position of the drop-down button */
    width: 20px; /* Width of the drop-down button */
    border-left: none; /* No left border */
}

#dateEdit::down-arrow {
   
    width: 10px; /* Width of the icon */
    height: 10px; /* Height of the icon */
}

#dateEdit QCalendarWidget {
    border: 1px solid #ccc; /* Use the same border color */
    background-color: #ffffff; /* White background for the calendar */
    selection-background-color: #0078d7; /* Blue background for selected date */
    selection-color: #ffffff; /* White text for selected date */
    padding: 5px; /* Padding around the calendar */
    border-radius: 8px; /* Match border radius with comboBox_3 */
    margin: 0px; /* Consistent margin */
}

/* Customize the items in the calendar */
#dateEdit QCalendarWidget QAbstractItemView {
    border: none; /* No border for the items */
    selection-background-color: #0078d7; /* Selected item background */
    selection-color: #ffffff; /* Selected item text color */
}

/* Styling for individual calendar days */
#dateEdit QCalendarWidget QAbstractItemView::item {
    padding: 8px; /* Padding for calendar items */
    text-align: center; /* Center-align the text */
    margin: 0px; /* Consistent margin */
}
